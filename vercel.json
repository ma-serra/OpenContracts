{
  "version": 2,
  "name": "opencontracts-frontend",
  "buildCommand": "cd frontend && yarn install && yarn build",
  "outputDirectory": "frontend/dist",
  "installCommand": "cd frontend && yarn install",
  "framework": "vite",
  "routes": [
    {
      "src": "/env-config.js",
      "dest": "/api/env-config"
    },
    {
      "src": "/((?!api/).*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/env-config.js": {
      "runtime": "@vercel/node@20"
    }
  }
}